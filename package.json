{
  "name": "service-json-keys-project",
  "version": "2.1.4",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-novel/service-json-keys.git"
  },
  "license": "AGPL-3.0",
  "type": "module",
  "scripts": {
    "format": "pnpm run format:prettier",
    "format:prettier": "prettier --write . --config prettier.config.ts",
    "preinstall": "npx only-allow pnpm",
    "lint": "pnpm lint:stylecheck && pnpm lint:typecheck && pnpm lint:eslint",
    "lint:eslint": "eslint .",
    "lint:stylecheck": "prettier --check . --config prettier.config.ts",
    "lint:typecheck": "tsc --noEmit",
    "prepublish:doc": "sed -Ei \"s|(a-novel/service-json-keys/[^/]+)v[0-9.]+|\\1v$(node -p -e \"require('./package.json').version\")|g\" README.md && git commit -a --amend --no-edit",
    "publish:major": "pnpm version major && pnpm prepublish:doc && git push --follow-tags",
    "publish:minor": "pnpm version minor && pnpm prepublish:doc && git push --follow-tags",
    "publish:patch": "pnpm version patch && pnpm prepublish:doc && git push --follow-tags"
  },
  "devDependencies": {
    "@a-novel-kit/nodelib-config": "^1.3.1",
    "@eslint/js": "^9.39.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^17.0.0",
    "jiti": "^2.6.1",
    "js-yaml": "^4.1.1",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^3.0.0",
    "prettier-plugin-sql": "^0.19.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4"
  },
  "packageManager": "pnpm@10.28.1",
  "engines": {
    "node": ">=23.11.0",
    "npm": ">=10.9.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
