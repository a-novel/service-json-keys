{
  "name": "service-json-keys-project",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/a-novel/service-json-keys.git"
  },
  "license": "AGPL-3.0",
  "type": "module",
  "scripts": {
    "format": "pnpm run format:prettier",
    "format:prettier": "prettier --write . --config prettier.config.ts",
    "preinstall": "npx only-allow pnpm",
    "lint": "pnpm lint:stylecheck && pnpm lint:typecheck && pnpm lint:eslint",
    "lint:eslint": "pnpm dlx eslint .",
    "lint:stylecheck": "prettier --check . --config prettier.config.ts",
    "lint:typecheck": "tsc",
    "publish:major": "pnpm version major && git push --follow-tags",
    "publish:minor": "pnpm version minor && git push --follow-tags",
    "publish:patch": "pnpm version patch && git push --follow-tags"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-sql": "^0.19.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4"
  },
  "packageManager": "pnpm@10.22.0",
  "engines": {
    "node": ">=23.11.0",
    "npm": ">=10.9.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
